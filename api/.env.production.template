# Production Environment Configuration Template
# Copy this file to .env.production and update the values

# Database Configuration
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_DATABASE=poxiao_db
MYSQL_USER=poxiao
MYSQL_PASSWORD=poxiao123

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=redis123

# Application Settings
APP_DOMAIN=https://your-domain.com
API_DOMAIN=https://api.your-domain.com
APP_DOMAIN_PC=https://your-domain.com
APP_DOMAIN_APP=https://app.your-domain.com
APP_PUSH_URL=https://push.your-domain.com
SYSTEM_PATH=/app/resources

# Security
JWT_SECRET=your-super-secret-jwt-key-minimum-32-characters
JWT_ISSUER=PoxiaoSystem
JWT_AUDIENCE=PoxiaoUsers
JWT_EXPIRE_MINUTES=120

# OAuth Settings (if enabled)
OAUTH_ENABLED=false
OAUTH_LOGIN_PATH=https://your-domain.com/api/oauth/Login
OAUTH_SUCCESS_URL=https://your-domain.com/sso
OAUTH_DEFAULT_SSO=auth2
OAUTH_TICKET_TIMEOUT=5
OAUTH_TICKET_MESSAGE=false

# Social Login (if enabled)
SOCIALS_ENABLED=false

# Error Reporting
ERROR_REPORT=false
ERROR_REPORT_TO=admin@your-domain.com

# Logging
LOG_LEVEL=Information

# Message Queue (if enabled)
EVENTBUS_ENABLED=false
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_VHOST=/

# AI Services
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_API_KEY=
QDRANT_USE_HTTPS=false

TEI_HOST=localhost
TEI_PORT=8080
TEI_MODEL=bge-m3

VLLM_HOST=localhost
VLLM_PORT=8000
VLLM_MODEL=qwen2.5-7b
VLLM_API_KEY=